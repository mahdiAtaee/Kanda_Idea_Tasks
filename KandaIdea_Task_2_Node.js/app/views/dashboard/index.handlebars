<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>صفحه کاربری</title>
    <link
      rel="shortcut icon"
      href="./assets/img/Logo1.png"
      type="image/x-icon"
    />
    <link rel="stylesheet" href="assets/styles/style.css" />
    <link rel="stylesheet" href="assets/styles/dashboard.css" />
  </head>
  <body>
    <div class="wrapper">
      <div class="left-section info">
        <div class="title">اطلاعات کاربری</div>
        <div class="item-wrapper">
          <label>نام:</label>
          <span class="firstName">{{data.first_name}}</span>
        </div>
        <div class="item-wrapper">
          <label>نام خانوادگی:</label>
          <span class="lastName">{{data.last_name}}</span>
        </div>
        <div class="item-wrapper">
          <label>نام کاربری:</label>
          <span class="username">{{data.username}}</span>
        </div>
        <div class="item-wrapper">
          <label>ایمیل:</label>
          <span class="email">{{data.email}}</span>
        </div>
        <div class="item-wrapper">
          <label>شماره تماس:</label>
          <span class="phoneNumber">{{data.phone_number}}</span>
        </div>
      </div>
      <div class="right-section control">
        <div class="user-avatar">
          <img src="./assets/img/user.png" class="img-fluid" alt="User" />
        </div>
        <a href="/" class="logout">
          <img src="./assets/img/logout.png" alt="Logout" class="img-fluid" />
        </a>
      </div>
    </div>
    <script src="./assets/scripts/jquery-3.3.1.min.js"></script>
    <script src="assets/scripts/Dashboard.js"></script>
  </body>
</html>